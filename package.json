{
  "name": "vscode-datree",
  "displayName": "VSCode Datree",
  "description": "Simple VSCode Extension that allows you to run Datree tests",
  "publisher": "suyashsonawane",
  "author": {
    "name": "Suyash Sonawane",
    "email": "suyashsonawane007@gmail.com",
    "url": "https://suyashsonawane.github.io/"
  },
  "homepage": "https://github.com/SuyashSonawane/vscode-datree",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuyashSonawane/vscode-datree"
  },
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.62.0"
  },
  "icon": "media/icon.png",
  "categories": [
    "Other",
    "Debuggers",
    "Formatters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "keywords": [
    "datree",
    "K8s",
    "helm"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-datree-view",
          "title": "VSCode Datree",
          "icon": "media/logo.png"
        }
      ]
    },
    "views": {
      "vscode-datree-view": [
        {
          "type": "webview",
          "id": "vscode-datree-sidebar",
          "name": "VSCode Datree",
          "icon": "media/logo.png",
          "contextualTitle": "VSCode Datree"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "flat": "^5.0.2",
    "js-yaml": "^4.1.0",
    "lookpath": "^1.2.2"
  }
}
